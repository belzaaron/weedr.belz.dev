{
    "name": "belzaaron/weedr.belz.dev",
    "description": "Small service to provide Zipcode to Forecast7 location URLs.",
    "keywords": ["weather", "widget", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "laravel/lumen-framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
